#include <cs50.h>
#include <stdio.h>

int greedy(int n);
int main(void)
{
	int n;
	do
	{
		n = get_int("Change owed: ");
		if (n >= 0)
		{

			printf("%i\n", greedy(n));
		}
	}
	while (n < 0);

}

int greedy(int n)
{
	int counter = 0;
	while (n != 0)
	{
		if (n >= 25)
		{
			n -= 25;
		}
		else if (n >= 10)
		{
			n -= 10;
		}
		else if (n >= 5)
		{
			n -= 5;
		}
		else if (n >= 1)
		{
			n -= 1;
		}
		counter ++;
	}
	return counter;
}
